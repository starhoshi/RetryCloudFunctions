{
  "name": "retrycf",
  "version": "0.0.8",
  "description": "retry cloud functions",
  "main": "out/retrycf.js",
  "types": "out/retrycf.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tslint -p tslint.json && tsc -d",
    "npm_publish": "yarn run build && npm version patch && git commit -am 'publish' && git push origin master && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/starhoshi/retrycf.git"
  },
  "keywords": [
    "firebase"
  ],
  "author": "star__hoshi",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.0.0",
    "jest": "^22.1.4",
    "ts-jest": "^22.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  },
  "jest": {
    "transform": {
      "\\.ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src/__tests__/.*.test.ts",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "dependencies": {
    "firebase-admin": "^5.8.1",
    "firebase-functions": "^0.8.1"
  }
}
